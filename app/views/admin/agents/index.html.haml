.create_new
  %li= link_to "Add an Agent", [:new, :admin, :agent]

%table
  %tr
    %th Name
    %th
    %th
  - agents.each do |agent|
    %tr
      %td= agent.name
      %td= link_to "edit", edit_admin_agent_path(agent)
      %td= link_to "delete", admin_agent_path(agent), method: :delete, class: 'delete',
        data: { confirm: 'Are you sure you want to delete this agent?' }
